{
    "version": 2,
    "builds": [
        {
            "src": "src/main.js",
            "use": "@vercel/node"
        }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "src/main.js",
            "methods": [
                "GET",
                "POST",
                "PUT",
                "DELETE",
                "OPTIONS"
            ],
            "headers": [
                {
                    "source": "/(.*)",
                    "headers": [
                        {
                            "key": "Access-Control-Allow-Origin",
                            "value": "cisc-alumni-frontend.vercel.app"
                        },
                        {
                            "key": "Access-Control-Allow-Methods",
                            "value": "GET, POST, OPTIONS"
                        },
                        {
                            "key": "Access-Control-Allow-Headers",
                            "value": "Content-Type, Authorization"
                        },
                        {
                            "key": "Access-Control-Allow-Credentials",
                            "value": "true"
                        }
                    ]
                }
            ]
        }
    ],
    "env": {
        "NODE_ENV": "production"
    }
}